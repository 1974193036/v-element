{
  "name": "@shishuaiyun/element",
  "type": "module",
  "version": "1.0.1",
  "description": "Vue3 components library",
  "keywords": [
    "Component",
    "UI",
    "Vue3"
  ],
  "license": "MIT",
  "author": "Shuaiyun Shi",
  "homepage": "https://github.com/1974193036/v-element",
  "main": "./dist/umd/v-element.umd.cjs",
  "module": "./dist/es/v-element.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/es/v-element.js",
      "require": "./dist/v-element.umd.cjs",
      "types": "./dist/types/index.d.ts"
    },
    "./dist/": {
      "import": "./dist/",
      "require": "./dist/"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "dist/index.css"
  ],
  "scripts": {
    "dev": "vite",
    "build:old": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "build-umd": "vite build --config vite.umd.config.ts",
    "build-es": "vite build --config vite.es.config.ts",
    "build-lib": "run-p build-es build-umd",
    "move-style": "move-file dist/es/index.css dist/index.css",
    "build": "npm run build-lib && npm run move-style",
    "prepublishOnly": "npm run build",
    "publish:lib": "npm publish --access=public",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "peerDependencies": {
    "vue": "^3.3.11"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/vue-fontawesome": "^3.0.6",
    "@popperjs/core": "^2.11.8",
    "async-validator": "^4.2.5",
    "axios": "^1.6.7",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@mistjs/eslint-config-vue": "^0.1.0",
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^18.19.3",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vitepress-demo-preview/component": "^2.3.2",
    "@vitepress-demo-preview/plugin": "^1.2.2",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.4",
    "@vue/tsconfig": "^0.5.0",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^24.0.0",
    "move-file-cli": "^3.0.0",
    "npm-run-all2": "^6.1.1",
    "postcss-color-mix": "^1.1.0",
    "postcss-each": "^1.1.0",
    "postcss-each-variables": "^0.3.0",
    "postcss-for": "^2.1.1",
    "postcss-nested": "^6.0.1",
    "typescript": "~5.3.0",
    "vite": "^5.0.10",
    "vite-plugin-dts": "^3.7.2",
    "vitepress": "1.0.0-rc.41",
    "vitest": "^1.2.2",
    "vue": "^3.3.11",
    "vue-tsc": "^1.8.25"
  }
}
